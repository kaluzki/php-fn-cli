{
  "name": "php-fn/cli",
  "license": "MIT",
  "homepage": "https://github.com/php-fn/php-fn-cli",
  "require": {
    "php-fn/di": "^2.0",
    "symfony/console": "^3.4",
    "phpdocumentor/reflection-docblock": "^4.3",

    "ext-json": "*",
    "php": ">=7.1"
  },
  "require-dev": {
    "php-fn/test": "^2.0",
    "php-coveralls/php-coveralls": "^2.1",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "fn\\": ["src/fn"]
    },
    "files": [
      "src/autoload-files.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "fn\\": ["tests/fn"]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}

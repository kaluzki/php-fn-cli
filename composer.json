{
  "name": "php-fn/cli",
  "license": "MIT",
  "homepage": "https://github.com/php-fn/php-fn-cli",
  "prefer-stable": true,
  "require": {
    "php-fn/di": "^1.1",
    "symfony/console": "^3",
    "php": ">=7.1"
  },
  "require-dev": {
    "php-fn/test": "^1",
    "docker-xampp/bin": "^1",
    "phpdocumentor/reflection-docblock": "^4",
    "roave/security-advisories": "dev-master"
  },
  "suggest": {
    "phpdocumentor/reflection-docblock": "to detect command and argument descriptions from doc blocks"
  },
  "autoload": {
    "psr-4": {
      "fn\\": ["src/fn"]
    },
    "files": [
      "src/autoload-files.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "fn\\": ["tests/fn"]
    }
  },
  "extra": {
    "di-config": {"wiring": "reflection"}
  }
}

{
  "name": "php-fn/cli",
  "license": "MIT",
  "homepage": "https://github.com/php-fn/php-fn-cli",
  "authors": [{"name": "Demjan Kaluzki"}],
  "prefer-stable": true,
  "require": {
    "php-fn/di": "^1",
    "symfony/console": "^3",

    "php": ">=7.0"
  },
  "require-dev": {
    "php-fn/test": "^1",

    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "fn\\": ["src/fn"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "fn\\": ["tests/fn"]
    }
  },
  "extra": {
    "di-config": {"wiring": "reflection"}
  }
}
